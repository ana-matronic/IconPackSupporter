<theme>

    <!-- The information used by GO Launcher. -->

    <version>
1
    </version>

    <!-- The theme name. -->

    <themeName>
Icon Pack
    </themeName>

    <!-- The theme information/description. -->

    <themeInfo>
Theme description.
    </themeInfo>

    <!--
    The preview drawables. You can list more by just by adding img4="theme_preview3" and so on.
    The first image is duplicated so that it is shown larger when the user opens the theme info
    because GO uses the first image only in the small preview in the theme list.
    -->

    <preview
        img1="theme_preview1"
        img2="theme_preview1"
        img3="theme_preview2" />

</theme>